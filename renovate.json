{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "timezone": "Europe/Berlin",
  "addLabels": [
    "dependencies"
  ],
  "prConcurrentLimit": 1,
  "commitMessagePrefix": "chore(deps): ",
  "nuget": {
    "packageRules": [
      {
        "groupName": "minor and patch updates",
        "groupSlug": "minor-and-patch",        
        "matchPackageNames": [
          "*"
        ],
        "matchUpdateTypes": [
          "minor",
          "patch"
        ]
      }
    ]
  },
  "automerge": false,
  "packageRules": [
  ]
}
