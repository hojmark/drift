{
  "profiles": {
    "init (interactive)": {
      "commandName": "Project",
      "commandLineArgs": "init",
      "dotnetRunMessages": true,
      "environmentVariables": {}
    },
    "init (non-interactive)": {
      "commandName": "Project",
      "commandLineArgs": "init mynetwork --overwrite -o log",
      "dotnetRunMessages": true,
      "environmentVariables": {}
    },
    "scan": {
      "commandName": "Project",
      "commandLineArgs": "scan",
      "dotnetRunMessages": true,
      "environmentVariables": {}
    },
    "scan ~": {
      "commandName": "Project",
      "commandLineArgs": "scan ~ -v",
      "dotnetRunMessages": true,
      "environmentVariables": {}
    },
    "scan ~ -i": {
      "commandName": "Project",
      "commandLineArgs": "scan ~ -i",
      "dotnetRunMessages": true,
      "environmentVariables": {}
    },
    "scan (o=log)": {
      "commandName": "Project",
      "commandLineArgs": "scan -o log",
      "dotnetRunMessages": true,
      "environmentVariables": {}
    },
    "lint": {
      "commandName": "Project",
      "commandLineArgs": "lint",
      "dotnetRunMessages": true,
      "environmentVariables": {}
    },
    "lint (home)": {
      "commandName": "Project",
      "commandLineArgs": "lint ~",
      "dotnetRunMessages": true,
      "environmentVariables": {}
    },
    "agent start (home)": {
      "commandName": "Project",
      "commandLineArgs": "agent start ~ -vv -o log --adoptable",
      "dotnetRunMessages": true,
      "environmentVariables": {}
